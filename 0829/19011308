def factorial(num): 
    if num < 0: 
        print("Factorial no existe")

    elif num == 0: 
        return 1
        
    else: 
        fact = 1
        while(num > 1): 
            fact *= num 
            num -= 1
        return fact 

numero = eval(input("Escribe un número: ")) 

print("El factorial de", numero, "es", factorial(numero))

