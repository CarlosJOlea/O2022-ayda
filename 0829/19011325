num= eval(input("Escribe un número: "))
def factorial(num): 
    
   
    if num < 0: 
        print("Factorial negativa no existe")
        

    elif num == 0: 
        return 1
        
    else: 
        fact = 1
        while(num > 1): 
            fact *= num 
            num -= 1
        return fact 



print(" El Factorial ",num,"es", factorial(num)) 
